            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.StockImportExcel">
    <children>
        <Label layoutX="175.0" layoutY="10.0" text="STOCK ADDING BY EXCEL OR .CSV SHEET">
            <font>
                <Font name="System Bold" size="17.0" />
